{
  "manifest_version": 3,
  "name": "Remembrance",
  "version": "1.0",
  "description": "Get reminded 7 days in advance of your loved ones' birthdays and anniversaries.",
  "permissions": ["activeTab"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/32.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "96": "icons/96.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
